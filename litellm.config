model_list:
  - model_name: OpenAI/gpt-4o-mini
    litellm_params:
      model: openai/gpt-4o-mini
      api_key: os.environ/OPENAI_API_KEY

  - model_name: OpenAI/gpt-4.1-nano
    litellm_params:
      model: openai/gpt-4.1-nano
      api_key: os.environ/OPENAI_API_KEY

  - model_name: OpenAI/gpt-4o-mini-transcribe
    litellm_params:
      model: openai/gpt-4o-mini-transcribe
      api_key: os.environ/OPENAI_API_KEY

  - model_name: OpenAI/whisper-1
    litellm_params:
      model: openai/whisper-1
      api_key: os.environ/OPENAI_API_KEY

  - model_name: OpenAI/gpt-4o-audio-preview # OpenAI gpt-4o-audio-preview
    litellm_params:
      model: openai/gpt-4o-audio-preview
      api_key: os.environ/OPENAI_API_KEY

  - model_name: Anthropic/claude-3-7-sonnet-latest
    litellm_params:
      model: anthropic/claude-3-7-sonnet-latest
      api_key: os.environ/ANTHROPIC_API_KEY

  - model_name: SambaNova/Meta-Llama-3.1-8B-Instruct
    litellm_params:
      model: sambanova/Meta-Llama-3.1-8B-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/Meta-Llama-3.2-3B-Instruct
    litellm_params:
      model: sambanova/Meta-Llama-3.2-3B-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/Meta-Llama-3.3-70B-Instruct
    litellm_params:
      model: sambanova/Meta-Llama-3.3-70B-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/Llama-3.3-Swallow-70B-Instruct-v0.4
    litellm_params:
      model: sambanova/Llama-3.3-Swallow-70B-Instruct-v0.4
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/QwQ-32B
    litellm_params:
      model: sambanova/QwQ-32B
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/DeepSeek-R1
    litellm_params:
      model: sambanova/DeepSeek-R1
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/DeepSeek-R1-Distill-Llama-70B
    litellm_params:
      model: sambanova/DeepSeek-R1-Distill-Llama-70B
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/DeepSeek-V3-0324
    litellm_params:
      model: sambanova/DeepSeek-V3-0324
      api_key: os.environ/SAMBANOVA_API_KEY

  - model_name: SambaNova/Llama-4-Scout-17B-16E-Instruct
    litellm_params:
      model: sambanova/Llama-4-Scout-17B-16E-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY
    model_info:
      supports_vision: True        # set supports_vision to True so /model/info returns this attribute as True

  - model_name: SambaNova/Llama-4-Maverick-17B-128E-Instruct
    litellm_params:
      model: sambanova/Llama-4-Maverick-17B-128E-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY
    model_info:
      supports_vision: True        # set supports_vision to True so /model/info returns this attribute as True

  - model_name: SambaNova/Whisper-Large-v3
    litellm_params:
      model: sambanova/Whisper-Large-v3
      api_key: os.environ/SAMBANOVA_API_KEY
    model_info:
      supports_audio_input: True        # set supports_audio_input to True so /model/info returns this attribute as True

  - model_name: SambaNova/Qwen2-Audio-7B-Instruct
    litellm_params:
      model: sambanova/Qwen2-Audio-7B-Instruct
      api_key: os.environ/SAMBANOVA_API_KEY
    model_info:
      supports_audio_input: True        # set supports_audio_input to True so /model/info returns this attribute as True

  - model_name: Google/gemini-2.0-flash
    litellm_params:
      model: gemini/gemini-2.0-flash-exp
      api_key: os.environ/GEMINI_API_KEY
    model_info:
      supports_vision: True        # set supports_vision to True so /model/info returns this attribute as True

  - model_name: Google/gemini-2.0-flash-exp-image-generation
    litellm_params:
      model: gemini/gemini-2.0-flash-exp
      api_key: os.environ/GEMINI_API_KEY
    model_info:
      supports_vision: True        # set supports_vision to True so /model/info returns this attribute as True
      supports_audio_input: True        # set supports_audio_input to True so /model/info returns this attribute as True
      supports_audio_output: True        # set supports_audio_input to True so /model/info returns this attribute as True

  - model_name: Ollama/llama3.2
    litellm_params:
      model: ollama/llama3.2:latest
      api_key: ""

  - model_name: Ollama/hermes3
    litellm_params:
      model: ollama/hermes3:latest
      api_key: ""

  - model_name: Ollama/qwen2.5:32b
    litellm_params:
      model: ollama/qwen2.5:32b
      api_key: ""

  # 画像生成モデル
  - model_name: OpenAI/dall-e-3
    litellm_params:
      model: openai/dall-e-3
      api_key: os.environ/OPENAI_API_KEY
    model_info:
      supports_image_generation: True

  # Text-to-Speech モデル
  - model_name: OpenAI/tts-1
    litellm_params:
      model: openai/tts-1
      api_key: os.environ/OPENAI_API_KEY
    model_info:
      supports_tts: True

  - model_name: OpenAI/tts-1-hd
    litellm_params:
      model: openai/tts-1-hd
      api_key: os.environ/OPENAI_API_KEY
    model_info:
      supports_tts: True

# ログ設定
log_config:
  request_log_path: ./logs
  log_level: debug
  log_requests: true
  log_responses: true
  verbose: true

# リクエスト/レスポンスのトラッキング設定
callbacks:
  - callback_type: local_log
    options:
      request_log_path: ./logs
